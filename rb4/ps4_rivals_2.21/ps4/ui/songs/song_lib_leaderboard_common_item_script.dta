(define SongLibLeaderboardItemCommon 
   (description "ScriptCom for common portion of the song library leaderboard items.") 
   (props 
      (player_name symbol 
         (default "PLAYER_NAME") 
         (set_on_instance TRUE)) 
      (player_icon_url symbol 
         (default "") 
         (set_on_instance TRUE)) 
      (row_num_int int 
         (default 0) 
         (set_on_instance TRUE)) 
      (score int 
         (default 0) 
         (set_on_instance TRUE)) 
      (rank_points int 
         (default 0) 
         (set_on_instance TRUE)) 
      (score_percentage_preformatted_string symbol 
         (default "0%") 
         (set_on_instance TRUE)) 
      (play_difficulty int 
         (default 0) 
         (set_on_instance TRUE) 
         (allowed_values 
            (
               (0 "Easy" "Easy") 
               (1 "Medium" "Medium") 
               (2 "Hard" "Hard") 
               (3 "Expert" "Expert")))) 
      (have_fc bool 
         (default FALSE) 
         (set_on_instance TRUE)) 
      (is_you bool 
         (default FALSE) 
         (set_on_instance TRUE)) 
      (is_brutal_fc bool 
         (default FALSE) 
         (set_on_instance TRUE)) 
      (fc_difficulty int 
         (default 0) 
         (set_on_instance TRUE) 
         (allowed_values 
            (
               (0 "Easy" "Easy") 
               (1 "Medium" "Medium") 
               (2 "Hard" "Hard") 
               (3 "Expert" "Expert"))))))
