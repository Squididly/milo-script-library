(define SongLibDifficultyDisplay 
   (description "ScriptCom for difficulty display entity in the song library.") 
   (props 
      (difficulty int 
         (default 0) 
         (set_on_instance TRUE) 
         (allowed_values 
            (
               (0 "Easy" "Easy") 
               (1 "Medium" "Medium") 
               (2 "Hard" "Hard") 
               (3 "Expert" "Expert"))))))
