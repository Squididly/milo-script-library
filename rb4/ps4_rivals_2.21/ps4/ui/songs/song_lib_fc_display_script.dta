(define SongLibFullComboDisplay 
   (description "ScriptCom for full combo display entity in the song library.") 
   (props 
      (have_fc bool 
         (default FALSE) 
         (set_on_instance TRUE)) 
      (is_brutal_fc bool 
         (default FALSE) 
         (set_on_instance TRUE)) 
      (fc_difficulty int 
         (default 0) 
         (set_on_instance TRUE) 
         (allowed_values 
            (
               (0 "Easy" "Easy") 
               (1 "Medium" "Medium") 
               (2 "Hard" "Hard") 
               (3 "Expert" "Expert"))))))
