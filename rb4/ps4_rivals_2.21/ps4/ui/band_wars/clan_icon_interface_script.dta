      (clan_badge_icon_url symbol 
         (set_on_instance TRUE) 
         (help "The URL path for the icon for this badge.") 
         (default )) 
      (match_master_user_on_enter bool 
         (set_on_instance TRUE) 
         (help "The URL path for the icon for this badge.") 
         (default FALSE)) 
      (display_alpha float 
         (set_on_instance TRUE) 
         (help "Alpha value of material of image.") 
         (default 1.0))))
