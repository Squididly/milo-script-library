(define Interface 
   (rev 0) 
   (description "Passes information from the RBSompHubCom to the active session instance.") 
   (was_interface TRUE) 
   (props 
      (invitor_username symbol 
         (set_on_instance TRUE) 
         (help "The gamertag of the invitation sender.")) 
      (invite_header symbol 
         (set_on_instance TRUE) 
         (help "The full localized text string describing the invitation.")) 
      (session_duration_hours int 
         (set_on_instance TRUE) 
         (help "How many hours the has session been running.") 
         (default 0)) 
      (session_duration_minutes int 
         (set_on_instance TRUE) 
         (help "How many minutes the has session been running.") 
         (default 0)) 
      (session_duration_seconds int 
         (set_on_instance TRUE) 
         (help "How many seconds the has session been running.") 
         (default 0)) 
      (session_num_active_players int 
         (set_on_instance TRUE) 
         (help "How many players are in the session.") 
         (default 0)) 
      (session_player_name_1 symbol 
         (set_on_instance TRUE) 
         (help "The name of the first player in the session.")) 
      (session_player_name_2 symbol 
         (set_on_instance TRUE) 
         (help "The name of the second player in the session.")) 
      (session_player_name_3 symbol 
         (set_on_instance TRUE) 
         (help "The name of the third player in the session.")) 
      (session_player_name_4 symbol 
         (set_on_instance TRUE) 
         (help "The name of the fourth player in the session.")) 
      (alpha float 
         (set_on_instance TRUE) 
         (help "Alpha propanim control.") 
         (min 0.0) 
         (max 1.0) 
         (default 0.0))))
