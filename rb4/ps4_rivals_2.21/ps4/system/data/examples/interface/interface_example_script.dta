(define Interface 
   (description "This is an example interface") 
   (was_interface TRUE) 
   (props 
      (float_prop float 
         (set_on_instance TRUE) 
         (default 1.0) 
         (help "Example float interface property")) 
      (int_prop int 
         (set_on_instance TRUE) 
         (default 2) 
         (help "Example int interface property")) 
      (bool_prop bool 
         (set_on_instance TRUE) 
         (default FALSE) 
         (help "Example bool interface property")) 
      (color_prop color 
         (set_on_instance TRUE) 
         (default 
            (1.0 1.0 1.0 1.0)) 
         (help "Example color interface property")) 
      (symbol_prop symbol 
         (set_on_instance TRUE) 
         (default Potato) 
         (allowed_values 
            (
               (Potato "Potato" "Best when fried") 
               (Rutabega "Rutabega" "Technically a food") 
               ('Bone Marrow' "Bone Marrow" "Most delicious!"))) 
         (help "Example symbol interface property")) 
      (object_prop object 
         (set_on_instance TRUE) 
         (help "Example object interface property"))))
