(define Interface 
   (rev 0) 
   (description "Provides an anchor for properties in panels with toggleable options.") 
   (props 
      (row_type int 
         (allowed_values 
            (
               (0 "Play" "Play Button.") 
               (1 "Mode" "Gameplay mode.") 
               (2 "Venue" "Venue.") 
               (3 "Difficulty" "Difficulty.") 
               (4 "Outfit" "Outfit.") 
               (5 "Credits" "Credits.") 
               (6 "Quit" "Quit.") 
               (7 "Tutorial" "Tutorial."))) 
         (set_on_instance TRUE)) 
      (fade_control float 
         (help "Control the alpha externally") 
         (set_on_instance TRUE))))
