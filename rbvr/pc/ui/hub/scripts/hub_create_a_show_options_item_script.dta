(define Interface 
   (rev 0) 
   (description "Provides an anchor for properties in the gameplay options panel ui list.") 
   (props 
      (row_type int 
         (allowed_values 
            (
               (0 "Show Length" "Show Length") 
               (1 "Venue" "Venue") 
               (2 "Outfit" "Outfit.") 
               (3 "Create" "Create.") 
               (4 "Difficulty" "Difficulty.") 
               (5 "Play" "Play.") 
               (6 "Performance Skill Level" "Performance Skill Level.") 
               (7 "Lefty Flip" "Lefty Flip."))) 
         (set_on_instance TRUE)) 
      (fade_control float 
         (help "Control the alpha externally") 
         (set_on_instance TRUE)) 
      (hub_options_panel_view int 
         (help "Use the view to drive some button text.") 
         (set_on_instance TRUE) 
         (allowed_values 
            (
               (0 "Campaign View" "") 
               (1 "Song Select View" "") 
               (2 "Settings View" "") 
               (3 "Create A Show View" "") 
               (4 "Confirm Settings View" ""))) 
         (default 0)) 
      (State int 
         (help "Use the view to drive some button position.") 
         (set_on_instance TRUE) 
         (allowed_values 
            (
               (0 "Create a Show" "") 
               (1 "Confirm Choices (Classic)" "") 
               (2 "Confirm Choices (Performance)" ""))) 
         (default 0))))
