(define Interface 
   (description "BottleInterface - Defines the behavior of bottles on the instance") 
   (props 
      (bottle_type int 
         (help "What kind of bottle is it - drives anim behavior") 
         (set_on_instance TRUE) 
         (default 0) 
         (allowed_values 
            (
               (0 "Fall Over" "Bottle falls over when hit.") 
               (1 "Launch" "Bottle gets launched when hit."))))))
