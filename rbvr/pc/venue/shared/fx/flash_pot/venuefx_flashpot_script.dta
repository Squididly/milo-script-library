(define Interface 
   (description "VR VenueFX Flashpots") 
   (props 
      (class_type int 
         (set_on_instance TRUE) 
         (allowed_values 
            (
               (0 "Single Fire") 
               (1 "Sustain")))) 
      (launch_fx bool 
         (set_on_instance TRUE) 
         (default FALSE) 
         (help "Enable to activate this flashpot")) 
      (rotate_disc bool 
         (view_on_instance TRUE) 
         (default FALSE) 
         (help "Flag to rotate flashpot disc")) 
      (enable_all_flashpots bool 
         (view_on_instance TRUE) 
         (default FALSE) 
         (help "Enable to activate all flashpots"))))
