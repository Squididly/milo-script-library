(define LaserEyesState 
   (description "Maintain some state specific to AlienLaserEyes in the campaign show.") 
   (props 
      (state int 
         (help "The current state of the prop") 
         (default 0) 
         (allowed_values 
            (
               (0 "Disabled" "") 
               (1 "Standby" "") 
               (2 "Active" "") 
               (3 "Blackout" "") 
               (4 "Evil" ""))) 
         (save FALSE))))
