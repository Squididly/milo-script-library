(define Interface 
   (description "Waypoint Config") 
   (props 
      (text_label int 
         (set_on_instance TRUE) 
         (help "") 
         (allowed_values 
            (
               (0 "off" "") 
               (1 "A" "") 
               (2 "B" "") 
               (3 "C" "")))) 
      (musician_active bool 
         (set_on_instance TRUE) 
         (default FALSE) 
         (help "Is a musician using this waypoint?")) 
      (waypoint_color color 
         (set_on_instance TRUE)) 
      (visibility_override bool 
         (set_on_instance TRUE) 
         (default FALSE) 
         (help "Should the waypoint be visible during Play Mode?"))))
